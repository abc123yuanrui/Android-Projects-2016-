<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="fill_parent" android:layout_height="fill_parent"
        xmlns:tools="http://schemas.android.com/tools" android:background="@drawable/background3">
<LinearLayout 
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:layout_gravity="center_vertical"
    
    tools:context="com.example.mylockapp.swipe_image_lock_set" >
    
    <include layout="@layout/header" android:layout_width="fill_parent" android:layout_height="@dimen/header_size"/>
    
<LinearLayout android:orientation="vertical" android:layout_gravity="center_vertical" android:gravity="center" android:layout_width="fill_parent" android:layout_height="fill_parent" >    
	<LinearLayout android:id="@+id/slide_image_lock_set_sub_main_ll" android:layout_width="fill_parent" android:layout_height="fill_parent" android:orientation="vertical" android:layout_gravity="center_vertical" android:gravity="center"  >
  		<LinearLayout android:id="@+id/slide_image_lock_set_sub_main" android:layout_width="wrap_content" android:layout_height="wrap_content" android:orientation="vertical" android:layout_gravity="center_horizontal" android:gravity="center"  android:background="#999999" android:paddingLeft="5dp" android:paddingTop="5dp" android:paddingRight="5dp" android:paddingBottom="5dp">
          
    <LinearLayout android:id="@+id/slide_image_lock_set_ll_1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:orientation="horizontal" android:layout_gravity="center_horizontal" android:gravity="center"  >
    	
    	<android.support.v4.view.ViewPager android:id="@+id/myviewpager_set" android:layout_width="@dimen/viewpager_size" android:layout_height="@dimen/viewpager_size" />
       
	    <View  android:layout_width="5dp" android:layout_height="@dimen/viewpager_size" android:background="#09dd00ff"></View>
    
    	<android.support.v4.view.ViewPager  android:id="@+id/myviewpager_2_set"  android:layout_width="@dimen/viewpager_size" android:layout_height="@dimen/viewpager_size" android:layout_marginLeft="0dp"/>
            
    	<View android:layout_width="5dp" android:layout_height="@dimen/viewpager_size" android:background="#09dd00ff"></View>
   
    	<android.support.v4.view.ViewPager android:id="@+id/myviewpager_3_set" android:layout_width="@dimen/viewpager_size" android:layout_height="@dimen/viewpager_size" android:layout_marginLeft="0dp" />
  	</LinearLayout>
  	<View android:layout_width="fill_parent" android:layout_height="5dp" android:background="#09dd00ff"></View>
	<LinearLayout android:id="@+id/slide_image_lock_set_ll_2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:orientation="horizontal" android:layout_gravity="center_horizontal" android:gravity="center">
        
    	<android.support.v4.view.ViewPager android:id="@+id/myviewpager_set_2" android:layout_width="@dimen/viewpager_size" android:layout_height="@dimen/viewpager_size" />
       
	    <View  android:layout_width="5dp" android:layout_height="@dimen/viewpager_size" android:background="#09dd00ff"></View>
    
    	<android.support.v4.view.ViewPager  android:id="@+id/myviewpager_2_set_2"  android:layout_width="@dimen/viewpager_size" android:layout_height="@dimen/viewpager_size" android:layout_marginLeft="0dp" />
            
    	<View android:layout_width="5dp" android:layout_height="@dimen/viewpager_size" android:background="#09dd00ff"></View>
   
    	<android.support.v4.view.ViewPager android:id="@+id/myviewpager_3_set_2" android:layout_width="@dimen/viewpager_size" android:layout_height="@dimen/viewpager_size" android:layout_marginLeft="0dp" />
  	</LinearLayout>
  	<View android:layout_width="fill_parent" android:layout_height="5dp" android:background="#09dd00ff"></View>
  	<LinearLayout android:id="@+id/slide_image_lock_set_ll_3" android:layout_width="wrap_content" android:layout_height="wrap_content" android:orientation="horizontal" android:layout_gravity="center_horizontal" android:gravity="center" >
        
    	<android.support.v4.view.ViewPager android:id="@+id/myviewpager_set_3" android:layout_width="@dimen/viewpager_size" android:layout_height="@dimen/viewpager_size" android:layout_gravity="center_vertical"/>
       
	    <View  android:layout_width="5dp" android:layout_height="@dimen/viewpager_size" android:background="#09dd00ff"></View>
    
    	<android.support.v4.view.ViewPager  android:id="@+id/myviewpager_2_set_3"  android:layout_width="@dimen/viewpager_size" android:layout_height="@dimen/viewpager_size" android:layout_marginLeft="0dp"/>
            
    	<View android:layout_width="5dp" android:layout_height="@dimen/viewpager_size" android:background="#09dd00ff"></View>
   
    	<android.support.v4.view.ViewPager android:id="@+id/myviewpager_3_set_3" android:layout_width="@dimen/viewpager_size" android:layout_height="@dimen/viewpager_size" android:layout_marginLeft="0dp" />
  	</LinearLayout>
  </LinearLayout>
   <View  android:layout_width="fill_parent" android:layout_height="30dp" android:background="@android:color/transparent"></View>
    <LinearLayout android:id="@+id/swipe_image_lock_set_button_ll" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_gravity="center_horizontal" android:orientation="horizontal" android:gravity="center" android:layout_marginBottom="30.0dp">
        
     <Button android:textSize="@dimen/sgalary_button_text_size" android:gravity="center" android:textColor="#ffffffff" android:layout_gravity="center_horizontal" android:id="@+id/swipe_image_lock_set_btcnl" android:background="@drawable/round_corner_white" android:layout_width="@dimen/sg_popup_dialog_width" android:layout_height="@dimen/sg_popup_dialog_height"  android:text="Cancle" />
     <View  android:layout_width="5dp" android:layout_height="10dp" android:background="@android:color/transparent"></View>     
     <Button android:textSize="@dimen/sgalary_button_text_size" android:gravity="center" android:textColor="#ffffffff" android:layout_gravity="center_horizontal" android:id="@+id/swipe_image_lock_set_btnconfirm" android:background="@drawable/round_corner_white" android:layout_width="@dimen/sg_popup_dialog_width" android:layout_height="@dimen/sg_popup_dialog_height"  android:text="Confirm" />
     <Button android:textSize="@dimen/sgalary_button_text_size" android:gravity="center" android:textColor="#ffffffff" android:layout_gravity="center_horizontal" android:id="@+id/swipe_image_lock_set_btnok" android:background="@drawable/round_corner_white" android:layout_width="@dimen/sg_popup_dialog_width" android:layout_height="@dimen/sg_popup_dialog_height"  android:text="Ok" />
     <View  android:layout_width="5dp" android:layout_height="10dp" android:background="@android:color/transparent"></View>    
   
    </LinearLayout>
    <LinearLayout android:id="@+id/image_select" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_gravity="center_horizontal" android:gravity="center" >
    <Button android:textSize="@dimen/sgalary_button_text_size" android:gravity="center" android:textColor="#ffffffff" android:layout_gravity="center_horizontal" android:id="@+id/swipe_image_lock_set_choose_image" android:background="@drawable/round_corner_white" android:layout_width="wrap_content" android:layout_height="@dimen/sg_popup_dialog_height"  android:text="Choose Images" android:paddingLeft="10dp" android:paddingRight="10dp" />
    </LinearLayout>
    <FrameLayout android:layout_width="fill_parent" android:layout_height="wrap_content">
    <LinearLayout android:id="@+id/hiden_panel" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_gravity="center_horizontal" android:gravity="center" android:background="#0000ff" android:paddingLeft="10dp" android:paddingTop="10dp" android:paddingRight="10dp" android:paddingBottom="10dp">
     
   <LinearLayout android:id="@+id/center_horizontal" android:layout_width="match_parent" android:layout_height="match_parent" android:orientation="horizontal" android:gravity="center" android:layout_gravity="center_horizontal">
	
	<LinearLayout android:id="@+id/center" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_gravity="center_vertical" android:gravity="center" android:orientation="vertical"   >
   	
   	<LinearLayout android:id="@+id/row1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_gravity="center_horizontal" android:gravity="center" android:orientation="horizontal"   android:paddingBottom="3dp">

   	    <Button android:textSize="@dimen/sg_popup_dialog_text_size" android:gravity="center"  android:layout_gravity="center_horizontal" android:id="@+id/box1" android:background="@drawable/round_corner_white" android:layout_width="@dimen/sg_popup_dialog_width" android:layout_height="@dimen/sg_popup_dialog_height" android:text="box 1"  />
   	    <View android:layout_width="3dp" android:layout_height="50dp" android:background="#0000ff"/>    
        <Button android:textSize="@dimen/sg_popup_dialog_text_size" android:gravity="center"  android:layout_gravity="center_horizontal" android:id="@+id/box2" android:background="@drawable/round_corner_white" android:layout_width="@dimen/sg_popup_dialog_width" android:layout_height="@dimen/sg_popup_dialog_height" android:text="box 2"  />
        <View android:layout_width="3dp" android:layout_height="50dp" android:background="#0000ff"/>
 		<Button android:textSize="@dimen/sg_popup_dialog_text_size" android:gravity="center"  android:layout_gravity="center_horizontal" android:id="@+id/box3" android:background="@drawable/round_corner_white" android:layout_width="@dimen/sg_popup_dialog_width" android:layout_height="@dimen/sg_popup_dialog_height" android:text="box 3"  />
    	    
   	</LinearLayout>

   	   	<LinearLayout android:id="@+id/row2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_gravity="center_horizontal" android:gravity="center" android:orientation="horizontal"  android:paddingBottom="3dp">

   	    <Button android:textSize="@dimen/sg_popup_dialog_text_size" android:gravity="center"  android:layout_gravity="center_horizontal" android:id="@+id/box4" android:background="@drawable/round_corner_white" android:layout_width="@dimen/sg_popup_dialog_width" android:layout_height="@dimen/sg_popup_dialog_height" android:text="box 4"  />
   	    <View android:layout_width="3dp" android:layout_height="50dp" android:background="#0000ff"/>    
        <Button android:textSize="@dimen/sg_popup_dialog_text_size" android:gravity="center"  android:layout_gravity="center_horizontal" android:id="@+id/box5" android:background="@drawable/round_corner_white" android:layout_width="@dimen/sg_popup_dialog_width" android:layout_height="@dimen/sg_popup_dialog_height" android:text="box 5"  />
        <View android:layout_width="3dp" android:layout_height="50dp" android:background="#0000ff"/>
 		<Button android:textSize="@dimen/sg_popup_dialog_text_size" android:gravity="center"  android:layout_gravity="center_horizontal" android:id="@+id/box6" android:background="@drawable/round_corner_white" android:layout_width="@dimen/sg_popup_dialog_width" android:layout_height="@dimen/sg_popup_dialog_height" android:text="box 6"  />
    	    
   	</LinearLayout>
   	   	<LinearLayout android:id="@+id/row3" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_gravity="center_horizontal" android:gravity="center" android:orientation="horizontal" >

   	    <Button android:textSize="@dimen/sg_popup_dialog_text_size" android:gravity="center"  android:layout_gravity="center_horizontal" android:id="@+id/box7" android:background="@drawable/round_corner_white" android:layout_width="@dimen/sg_popup_dialog_width" android:layout_height="@dimen/sg_popup_dialog_height" android:text="box 7"  />
   	    <View android:layout_width="3dp" android:layout_height="50dp" android:background="#0000ff"/>    
        <Button android:textSize="@dimen/sg_popup_dialog_text_size" android:gravity="center"  android:layout_gravity="center_horizontal" android:id="@+id/box8" android:background="@drawable/round_corner_white" android:layout_width="@dimen/sg_popup_dialog_width" android:layout_height="@dimen/sg_popup_dialog_height" android:text="box 8"  />
        <View android:layout_width="3dp" android:layout_height="50dp" android:background="#0000ff"/>
 		<Button android:textSize="@dimen/sg_popup_dialog_text_size" android:gravity="center"  android:layout_gravity="center_horizontal" android:id="@+id/box9" android:background="@drawable/round_corner_white" android:layout_width="@dimen/sg_popup_dialog_width" android:layout_height="@dimen/sg_popup_dialog_height" android:text="box 9"   />
    	    
   	</LinearLayout>
   	</LinearLayout>
</LinearLayout>  
  </LinearLayout>
 
    </FrameLayout>
    
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
</FrameLayout>